{
   "name":"dangoscomb/elastic-apm-laravel",
   "keywords":[  
      "laravel",
      "elastic",
      "APM",
      "Lumen",
      "monitoring",
      "debug"
   ],
   "description":"A package to integrate Elastic APM into Laravel",
   "license":"MIT",
   "require":{  
      "php":">= 7.4",
      "illuminate/database":"^7.0",
      "illuminate/http":"^7.0",
      "illuminate/routing":"^7.0",
      "illuminate/support":"^7.0",
      "nipwaayoni/elastic-apm-php-agent":">=7.5.2",
      "ramsey/uuid":"^3.0"
   },
   "require-dev":{  
      "phpunit/phpunit":"6.*"
   },
   "autoload":{  
      "psr-4":{  
         "DanGoscomb\\ElasticApmLaravel\\":"src/"
      }
   },
   "autoload-dev":{  
      "psr-4":{  
         "DanGiscinb\\Tests\\":"tests/"
      }
   },
   "extra":{  
      "laravel":{  
         "providers":[  
            "DanGoscomb\\ElasticApmLaravel\\Providers\\ElasticApmServiceProvider"
         ],
         "aliases":{  
            "ElasticApm":"DanGoscomb\\ElasticApmLaravel\\Facades\\ElasticApm"
         }
      }
   },
   "config":{  
      "optimize-autoloader":true
   },
   "authors":[  
      {  
         "name":"Philip Krauss",
         "email":"philip@philipkrauss.at",
         "homepage":"https://github.com/philkra/elastic-apm-laravel"
      },
      {  
         "name":"George Boot",
         "email":"george@entryninja.com"
      },
      {
         "name": "Dan Goscomb",
         "email": "dan@flowplex.co.uk"
      }
   ]
}
